-{"version":3,"sources":["../../src/serializer/html.ts","../../src/constants.ts","../../src/interfaces/heading.ts"],"sourcesContent":["import { HTMLSerializerWithTransform } from '@editablejs/serializer/html'\nimport { HeadingTags } from '../constants'\nimport { Heading } from '../interfaces/heading'\n\nexport const withHeadingHTMLSerializerTransform: HTMLSerializerWithTransform = (\n  next,\n  serializer,\n  customOptions = {},\n) => {\n  const { attributes: customAttributes, style: customStyle } = customOptions\n  return (node, options) => {\n    const { attributes, style } = options ?? {}\n    if (Heading.isHeading(node)) {\n      return serializer.create(\n        HeadingTags[node.type],\n        serializer.mergeOptions(node, attributes, customAttributes),\n        serializer.mergeOptions(node, style, customStyle),\n        node.children.map(child => serializer.transform(child)).join(''),\n      )\n    }\n    return next(node, options)\n  }\n}\n","export const HEADING_KEY = 'heading'\nexport const PARAGRAPH_KEY = 'paragraph'\nexport const HEADING_ONE_KEY = 'heading-one'\nexport const HEADING_TWO_KEY = 'heading-two'\nexport const HEADING_THREE_KEY = 'heading-three'\nexport const HEADING_FOUR_KEY = 'heading-four'\nexport const HEADING_FIVE_KEY = 'heading-five'\nexport const HEADING_SIX_KEY = 'heading-six'\n\nexport const HeadingTags = {\n  [HEADING_ONE_KEY]: 'h1',\n  [HEADING_TWO_KEY]: 'h2',\n  [HEADING_THREE_KEY]: 'h3',\n  [HEADING_FOUR_KEY]: 'h4',\n  [HEADING_FIVE_KEY]: 'h5',\n  [HEADING_SIX_KEY]: 'h6',\n}\n","import { Descendant, Element } from '@editablejs/models'\nimport { HeadingTags } from '../constants'\n\nexport type HeadingType = keyof typeof HeadingTags\n\nexport interface Heading extends Element {\n  type: HeadingType\n}\n\nexport type HeadingFontStyleName = 'fontSize' | 'fontWeight'\n\nexport type HeadingTextMark = Record<HeadingFontStyleName, string>\n\nexport const Heading = {\n  isHeading: (value: any): value is Heading => {\n    return Element.isE